<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF  
   xmlns:rdf ="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:xs="http://www.w3.org/2000/10/XMLSchema#" 
   xmlns:dc="http://purl.org/dc/elements/1.1/"  
   xmlns = "http://www.w3.org/2000/10/XMLSchema#"
   xml:base  ="http://ns.inria.fr/semsna/2009/06/21/voc" >


<owl:Ontology rdf:about="http://ns.inria.fr/semsna/2009/06/21/voc">
  <dc:title xml:lang="en">SemSNA Core Ontology Namespace</dc:title>
  <owl:versionInfo>Revision: 1.2</owl:versionInfo>
  <dc:description xml:lang="en">SemSNA (Semantic social network analysis) is an ontology for leveraging social data with social network analysis indices. </dc:description>
</owl:Ontology>


<!-- top concepts -->
<owl:Class rdf:ID="SNAConcept"> 
  <label xml:lang="en">SNA concept</label>
  <comment xml:lang="en">A super class for SNA Concepts</comment>
</owl:Class>

<owl:Class rdf:ID="SNAIndice"> 	
  <rdfs:subClassOf rdf:resource="#SNAConcept"/>
  <label xml:lang="en">SNA indice</label>
  <comment xml:lang="en">A super class for valued SNA Concepts</comment>
</owl:Class>

<owl:ObjectProperty rdf:ID="hasSNAConcept">
	<range rdf:resource="#SNAConcept"/>
	<label xml:lang="en">has SNA indice</label>
	<comment xml:lang="en">link a resource to a SNA Indice</comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="isDefinedForProperty">
	<domain rdf:resource="#SNAConcept"/>
	<range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<label xml:lang="en">is defined for property</label>
	<comment xml:lang="en">Describe the pattern this is applicable for a SNA Concept, used to parametrize a SNA concept</comment>
</owl:ObjectProperty>


<owl:ObjectProperty rdf:ID="hasValue">
  	<domain rdf:resource="#SNAIndice"/>
 	<range rdf:resource="http://www.w3.org/2000/10/XMLSchema#decimal"/>
	<label xml:lang="en">the value of a SNA Indice</label>
	<comment xml:lang="en">Indicate the value of a SNA indice</comment>
</owl:ObjectProperty>


<!-- centrality -->


<owl:Class rdf:ID="Centrality">
  	<rdfs:subClassOf rdf:resource="#SNAIndice"/>
	<label xml:lang="en">centrality</label>
	<comment xml:lang="en">a top concept for all centrality indices</comment>
</owl:Class>


<owl:ObjectProperty rdf:ID="hasCentralityDistance">
  	<domain rdf:resource="#Centrality"/>
 	<range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/>
	<label xml:lang="en">has centrality distance</label>
	<comment xml:lang="en">the distance of the neighborhood that is taken into account</comment>
</owl:ObjectProperty>


<owl:Class rdf:ID="Degree">
  	<rdfs:subClassOf rdf:resource="#Centrality"/>
	<label xml:lang="en">degree</label>
	<comment xml:lang="en">The degree of a resource</comment>
</owl:Class>


<owl:Class rdf:ID="InDegree">
	<rdfs:subClassOf rdf:resource="#Centrality"/>
	<label xml:lang="en">in degree</label>
	<comment xml:lang="en">The in degree of a resource</comment>
</owl:Class>



<owl:Class rdf:ID="OutDegree">
  	<rdfs:subClassOf rdf:resource="#Centrality"/>
	<label xml:lang="en">out degree</label>
	<comment xml:lang="en">The out degree of a resource</comment>
</owl:Class>

<owl:Class rdf:ID="ClosenessCentrality">
	<rdfs:subClassOf rdf:resource="#Centrality"/>
	<label xml:lang="en">closeness centrality of a resource </label>
	<comment xml:lang="en">the closeness centrality of a resource (the sum of its shortest path to other resources)</comment>
</owl:Class>

<owl:Class rdf:ID="BetweenessCentrality">
	<rdfs:subClassOf rdf:resource="#Centrality"/>
	<label xml:lang="en">betweenness centrality of a resource</label>
	<comment xml:lang="en">The betweenness centrality of a resource (the sum of its betweenness)</comment>
</owl:Class>

<owl:Class rdf:ID="Betweenness">
	<rdfs:subClassOf rdf:resource="#SNAIndice"/>
	<label xml:lang="en">betweenness of a resource</label>
	<comment xml:lang="en">The betwenness of a resource for a couple of resources</comment>
</owl:Class>

<rdf:Property rdf:ID="betweenessExtremity">
  	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	<domain rdf:resource="#Betweenness"/>
	<label xml:lang="en">betweeness extremity</label>
	<comment xml:lang="en">One of the resource extremity of a betweenness</comment>
</rdf:Property>



<!-- Groups-->
<owl:Class rdf:ID="Group">
	<rdfs:subClassOf rdf:resource="#SNAConcept"/>
	<label xml:lang="en">group</label>
	<comment xml:lang="en">A group of resources</comment>
</owl:Class>

<owl:Class  rdf:ID="Diameter">
  	<rdfs:subClassOf rdf:resource="#SNAIndice"/>
	<label xml:lang="en">diameter</label>
	<comment xml:lang="en">the diameter of a group of resources is the maximum shortest path between any two resources</comment>
</owl:Class>

<owl:Class rdf:ID="Modularity">
	<rdfs:subClassOf rdf:resource="#SNAIndice"/>
	<label xml:lang="en">modularity</label>
	<comment xml:lang="en">The modularity of a group of resources</comment>
</owl:Class>

<owl:ObjectProperty rdf:ID="hasMember">
	<owl:inverseOf rdf:resource="#memberOf"/>
	<domain rdf:resource="#Group"/>
	<label xml:lang="en">has member</label>
	<comment xml:lang="en">A group has a member</comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="memberOf">
	<domain rdf:resource="#Group"/>
 	<range rdf:resource="#Diameter"/>
	<label xml:lang="en">member of</label>
	<comment xml:lang="en">A resource is member of a group</comment>
</owl:ObjectProperty>

<owl:Class rdf:ID="Component">
	<rdfs:subClassOf rdf:resource="#Group"/>
	<label xml:lang="en">Component</label>
	<comment xml:lang="en">A component is group of connected resources</comment>
</owl:Class>


<owl:Class rdf:ID="StrongComponent">
	<rdfs:subClassOf rdf:resource="#Component"/>
	<label xml:lang="en">Component</label>
	<comment xml:lang="en">A Strong component is group of connected resources with no change in path directions</comment>
</owl:Class>



<owl:Class rdf:ID="KPlex">
	<rdfs:subClassOf rdf:resource="#Component"/>
	<label xml:lang="en">KPlex</label>
	<comment xml:lang="en">A kplex is a clique that accept resources connected to maximum k members with a higher distance than the maximum clique distance</comment>
</owl:Class>

<owl:Class rdf:ID="Clique">
	<rdfs:subClassOf rdf:resource="#KPlex"/>
	<label xml:lang="en">Clique</label>
	<comment xml:lang="en">A clique is a component of a given diameter (an n-clique is a clique with a diameter of n)</comment>
</owl:Class>

<rdf:Property rdf:ID="hasCliqueDistance">
  	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	<domain rdf:resource="#Group"/>
 	<range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/>
	<label xml:lang="en">clique distance</label>
	<comment xml:lang="en">hasCliqueDistance define the maximum shortest path between two resources in a clique</comment>
</rdf:Property>

<rdf:Property rdf:ID="numberExcept">
  	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	<domain rdf:resource="#KPlex"/>
 	<range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/>
	<label xml:lang="en">length</label>
	<comment xml:lang="en">numberExcept represent the maximum number of members of a kplex that a member can be link with a higher distance than the maximum required</comment>
</rdf:Property>

<owl:Class rdf:ID="NClan">
	<rdfs:subClassOf rdf:resource="#Clique"/>
	<label xml:lang="en">KPlex</label>
	<comment xml:lang="en">A nclan is a clique with a maximum shortest distance equal to its diameter</comment>
</owl:Class>

<owl:Class rdf:ID="ClusteringCoefficient">
	<rdfs:subClassOf rdf:resource="#SNAIndice"/>
	<label xml:lang="en">clustering coefficient</label>
	<comment xml:lang="en">the clustering coeffcient quantifies how close a resource and its neighbors are to being a clique</comment>
</owl:Class>



<!-- paths -->
<owl:Class rdf:ID="Path">
	<rdfs:subClassOf rdf:resource="#Group"/>
	<label xml:lang="en">a path</label>
	<comment xml:lang="en">a Path between two resources</comment>
</owl:Class>


<owl:Class rdf:ID="DirectedPath">
	<rdfs:subClassOf rdf:resource="#Path"/>
	<label xml:lang="en">a directed path</label>
	<comment xml:lang="en">a directed path from a resource resource to another</comment>
</owl:Class>


<owl:Class rdf:ID="GeodesicPath">
	<rdfs:subClassOf rdf:resource="#Path"/>
	<label xml:lang="en">a geodesic path</label>
	<comment xml:lang="en">The geodesic path between two resources</comment>
</owl:Class>

<owl:Class rdf:ID="CyclicPath">
	<rdfs:subClassOf rdf:resource="#Path"/>
	<label xml:lang="en">Cyclic path </label>
	<comment xml:lang="en">A cyclic path </comment>
</owl:Class>

<owl:Class rdf:ID="NonCyclicPath">
	<rdfs:subClassOf rdf:resource="#Path"/>
	<owl:disjointWith rdf:resource="#CyclicPath" />
	<label xml:lang="en">non-Cyclic path </label>
	<comment xml:lang="en">A non cyclic path</comment>
</owl:Class>


<owl:ObjectProperty rdf:ID="pathExtremity">
	<rdfs:subPropertyOf rdf:resource="#hasMember"/>
	<domain rdf:resource="#Path"/>
	<label xml:lang="en">path extremity</label>
	<comment xml:lang="en">One of the resource extremity of a path</comment>
</owl:ObjectProperty>



<owl:ObjectProperty rdf:ID="from">
	<rdfs:subPropertyOf rdf:resource="#pathExtremity"/>
	<domain rdf:resource="#DirectedPath"/>
	<label xml:lang="en">from</label>
	<comment xml:lang="en">the first resource of a directed path</comment>
</owl:ObjectProperty>


<owl:ObjectProperty rdf:ID="to">
  	<rdfs:subPropertyOf rdf:resource="#pathExtremity"/>
	<domain rdf:resource="#DirectedPath"/>
	<label xml:lang="en">to</label>
	<comment xml:lang="en">the last resource of a directed path</comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="hasPathLength">
  	<domain rdf:resource="#Path"/>
 	<range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/>
	<label xml:lang="en">length</label>
	<comment xml:lang="en">the length of a path</comment>
</owl:ObjectProperty>

<!-- Community, need to rely to Palette community of practice ontology -->
<owl:Class rdf:ID="Community">
	<rdfs:subClassOf rdf:resource="#Group"/>
	<label xml:lang="en">community</label>
	<comment xml:lang="en">A community is a group of people linked by social ties, share common perspectives and engage in joint action in geographical locations or settings</comment>
</owl:Class>


<owl:Class rdf:ID="InterestCommunity">
	<rdfs:subClassOf rdf:resource="#Community"/>
	<label xml:lang="en">interest community</label>
	<comment xml:lang="en">A community of interest is a gathering of people assembled around a topic of common interests</comment>
</owl:Class>


<owl:Class rdf:ID="GoalOrientedCommunity">
	<rdfs:subClassOf rdf:resource="#Community"/>
	<label xml:lang="en">goal oriented community</label>
	<comment xml:lang="en">A form of community created to meet specific needs, to solve a particular problem</comment>
</owl:Class>


<owl:Class rdf:ID="LearningCommunity">
	<rdfs:subClassOf rdf:resource="#Community"/>
	<label xml:lang="en">learning community</label>
	<comment xml:lang="en">A learning community is a group of learners that collaborate to learn something</comment>
</owl:Class>


<owl:Class rdf:ID="PracticeCommunity">
	<rdfs:subClassOf rdf:resource="#Community"/>
	<label xml:lang="en">identity community</label>
	<comment xml:lang="en">An identity community is a community with members that share the same practise</comment>
</owl:Class>


<owl:Class rdf:ID="EpistemicCommunity">
	<rdfs:subClassOf rdf:resource="#Community"/>
	<label xml:lang="en">epistemic community</label>
	<comment xml:lang="en">A group a person engaged in a collaborative cognitive process in order to produce knowledge</comment>
</owl:Class>

<owl:Class rdf:ID="IdentityCommunity">
	<rdfs:subClassOf rdf:resource="#Community"/>
	<label xml:lang="en">identity community</label>
	<comment xml:lang="en">A group of people that a share a strong feeling of membership to an identity, an assumed or claimed status or an opinion</comment>
</owl:Class>


</rdf:RDF>
